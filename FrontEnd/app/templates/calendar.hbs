<!-- This page is populated by calls to the logo-tile, day-tile, and coach-sidebar components. It forms the calendar page for the admins to see.-->
<!-- NEED DATABASE is a placeholder that will eventually be filled by calls to the API to fill those dates with classes-->

<section class ={{wrapper}}>
<calendarContainer>


    <calendarRow>
	<calendarViewMain>
        
		{{logo-tile logo=true}}
		{{day-tile header=true redDay = true day="Sunday"}}
		{{day-tile header=true day="Monday"}}
                {{day-tile header=true redDay = true day="Tuesday"}}
                {{day-tile header=true day="Wednesday"}}
                {{day-tile header=true redDay = true day="Thursday"}}
                {{day-tile header=true day="Friday"}}
                {{day-tile header=true redDay = true day="Saturday"}}

                
	</calendarViewMain>

        <calendarViewCoaches>
		{{coach-sidebar header = true}}
        </calendarViewCoaches>
	

        </calendarRow>
    
    
    <calendarRow>
        <calendarViewMain>
                {{#if (dayCheck (format-date 'dddd')  'Sunday') }}
                {{logo-tile logo=false date=(format-date) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date))   date=(format-date)}}
                {{day-tile                  course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1)}}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 2)) date=(format-date 'addTime' 2)}}
                {{day-tile                  course = (find-course calendar.data (format-date 'addTime' 3)) date=(format-date 'addTime' 3)}}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 4)) date=(format-date 'addTime' 4) }}
                {{day-tile                  course = (find-course calendar.data (format-date 'addTime' 5)) date=(format-date 'addTime' 5) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 6)) date=(format-date 'addTime' 6) }}


                {{else if (dayCheck (format-date 'dddd')  'Monday') }}
                {{logo-tile logo=false date=(format-date 'subtractTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile                course = (find-course calendar.data (format-date)) date=(format-date) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 2)) date=(format-date 'addTime' 2) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 3)) date=(format-date 'addTime' 3) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 4)) date=(format-date 'addTime' 4) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 5)) date=(format-date 'addTime' 5) }}

                {{else if (dayCheck (format-date 'dddd')  'Tuesday') }}
                {{logo-tile logo=false date=(format-date 'subtractTime' 2) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 2)) date=(format-date 'subtractTime' 2) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date)) date=(format-date) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 2)) date=(format-date 'addTime' 2) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 3)) date=(format-date 'addTime' 3) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 4)) date=(format-date 'addTime' 4) }}

                {{else if (dayCheck (format-date 'dddd')  'Wednesday')}}
                {{logo-tile logo=false date=(format-date 'subtractTime' 3)}}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 3)) date=(format-date 'subtractTime' 3) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 2)) date=(format-date 'subtractTime' 2) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile                course = (find-course calendar.data (format-date)) date=(format-date) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 2)) date=(format-date 'addTime' 2) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 3)) date=(format-date 'addTime' 3) }}



                {{else if (dayCheck (format-date 'dddd')  'Thursday') }}
                {{logo-tile logo=false date=(format-date 'subtractTime' 4) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 4)) date=(format-date 'subtractTime' 4) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 3)) date=(format-date 'subtractTime' 3) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 2)) date=(format-date 'subtractTime' 2) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date)) date=(format-date) }}
                {{day-tile                course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 2)) date=(format-date 'addTime' 2) }}

                {{else if (dayCheck (format-date 'dddd')  'Friday') }}
                {{logo-tile logo=false date=(format-date 'subtractTime' 5) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 5)) date=(format-date 'subtractTime' 5) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 4)) date=(format-date 'subtractTime' 4) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 3)) date=(format-date 'subtractTime' 3) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 2)) date=(format-date 'subtractTime' 2) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile                course = (find-course calendar.data (format-date)) date=(format-date) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'addTime' 1)) date=(format-date 'addTime' 1) }}

                {{else if (dayCheck (format-date 'dddd')  'Saturday') }}
                {{logo-tile logo=false date=(format-date 'subtractTime' 6) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 6)) date=(format-date 'subtractTime' 6) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 5)) date=(format-date 'subtractTime' 5) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 4)) date=(format-date 'subtractTime' 4) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 3)) date=(format-date 'subtractTime' 3) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date 'subtractTime' 2)) date=(format-date 'subtractTime' 2) }}
                {{day-tile                course = (find-course calendar.data (format-date 'subtractTime' 1)) date=(format-date 'subtractTime' 1) }}
                {{day-tile  redDay = true  course = (find-course calendar.data (format-date)) date=(format-date) }}

                {{/if}}

                </calendarViewMain>
        
        <calendarViewCoaches>
   
            {{coach-sidebar name ="Michael Ho" header = false}}
             {{coach-sidebar name="Joey Williams" header = false}}
             {{coach-sidebar name="Mike Jones" header = false}}
        </calendarViewCoaches>
	
		
	
    </calendarRow>
    
    
      <calendarRow>
          <calendarViewMain>
                {{logo-tile logo = false date=(format-date 'W2' 'Su') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W2' 'Su')) date=(format-date 'W2' 'Su') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W2' 'M')) date=(format-date 'W2' 'M') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W2' 'Tu')) date=(format-date 'W2' 'Tu') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W2' 'W')) date=(format-date 'W2' 'W') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W2' 'Th')) date=(format-date 'W2' 'Th') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W2' 'F')) date=(format-date 'W2' 'F') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W2' 'Sa')) date=(format-date 'W2' 'Sa') }}
                
                </calendarViewMain>
	
        <calendarViewCoaches>
            {{coach-sidebar removeCoach=true header = false}}
            {{coach-sidebar header = false}}
            {{coach-sidebar header = false}}
             
        </calendarViewCoaches>
          
      </calendarRow>
      <calendarRow>
          <calendarViewMain>
                {{logo-tile logo = false date=(format-date 'W3' 'Su') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W3' 'Su')) date=(format-date 'W3' 'Su') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W3' 'M')) date=(format-date 'W3' 'M') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W3' 'Tu')) date=(format-date 'W3' 'Tu') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W3' 'W')) date=(format-date 'W3' 'W') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W3' 'Th')) date=(format-date 'W3' 'Th') }}
                {{day-tile               course = (find-course calendar.data (format-date 'W3' 'F')) date=(format-date 'W3' 'F') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W3' 'Sa')) date=(format-date 'W3' 'Sa') }}
                
                </calendarViewMain>

        <calendarViewCoaches>
         
            {{coach-sidebar header = false}}
             {{coach-sidebar header = false}}
             {{coach-sidebar header = false}}
        </calendarViewCoaches>
      </calendarRow>
      <calendarRow>
          <calendarViewMain>
                {{logo-tile logo = false date=(format-date 'W4' 'Su') }}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W4' 'Su')) date=(format-date 'W4' 'Su')}}
                {{day-tile               course = (find-course calendar.data (format-date 'W4' 'M')) date=(format-date 'W4' 'M')}}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W4' 'Tu')) date=(format-date 'W4' 'Tu')}}
                {{day-tile               course = (find-course calendar.data (format-date 'W4' 'W')) date=(format-date 'W4' 'W')}}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W4' 'Th')) date=(format-date 'W4' 'Th')}}
                {{day-tile               course = (find-course calendar.data (format-date 'W4' 'F')) date=(format-date 'W4' 'F')}}
                {{day-tile redDay = true course = (find-course calendar.data (format-date 'W4' 'Sa')) date=(format-date 'W4' 'Sa')}}
                
                </calendarViewMain>
        <calendarViewCoaches>
    
            {{coach-sidebar header = false}}
             {{coach-sidebar header = false}}
             {{coach-sidebar header = false}}
        </calendarViewCoaches>
	  </calendarRow>
     
	</calendarContainer>
</section>